{
  "name": "@angshus/patient-search",
  "version": "1.0.0",
  "description": "A lightweight JavaScript library for searching and selecting patients with FHIR support",
  "type": "module",
  "main": "dist/patient-search-util.js",
  "module": "dist/patient-search-util.esm.js",
  "browser": "dist/patient-search-util.umd.js",
  "unpkg": "dist/patient-search-util.min.js",
  "style": "dist/patient-search-util.css",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "npm run clean && rollup -c --environment BUILD:production",
    "build:dev": "npm run clean && rollup -c --environment BUILD:development",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js test/**/*.js",
    "lint:fix": "eslint src/**/*.js test/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "serve": "http-server . -p 8080 -o",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "patient",
    "search",
    "fhir",
    "healthcare",
    "modal",
    "library",
    "vanilla-javascript",
    "iframe"
  ],
  "author": "Angshus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/angshus/js-search-patient.git"
  },
  "bugs": {
    "url": "https://github.com/angshus/js-search-patient/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "rollup": "^4.12.0",
    "rollup-plugin-postcss": "^4.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
